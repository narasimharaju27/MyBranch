<aura:component >

    <aura:attribute name="item" type="Camping_Item__c"/>

    <p>Name:
        <ui:outputText value="{!v.expense.Name}"/>
    </p>
    <p>Price:
        <ui:outputCurrency value="{!v.expense.Price__c}"/>
    </p>
    <p>Quantity:
        <ui:outputCurrency value="{!v.expense.Quantity__c}"/>
    </p>
    <p>Packed?:
        <ui:outputCheckbox value="{!v.expense.Packed__c}"/>
    </p>

</aura:component>